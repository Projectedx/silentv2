    for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
		if v.Name == "NameBbGui" then
			v:Destroy()
		end
	end


	for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
		if v.Name == "RankBbGui" then
			v:Destroy()
		end
	end
end)
