Section:NewToggle("Psychic All", "Soul attacks everyone in your proximity", function(state)
    if state then
        _G.on = 1
	while _G.on == 1
	do
		for i,v in pairs(game.Players:GetPlayers()) do
			pcall(function()
				game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_Start",game.Players[v.Name]})
				game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
			end)

			wait()
		end
	end

    else
        _G.on = 0
	while _G.on == 1
	do
		for i,v in pairs(game.Players:GetPlayers()) do
			pcall(function()
				game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_Start",game.Players[v.Name]})
				game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
			end)

			wait()
		end
	end
    end
end)
Section:NewToggle("Invisible", "Makes your character invisible all the time", function(state)
    if state then
        _G.on = 1
	while _G.on == 1
	do
		local args = {
			[1] = {
				[1] = "Skill_Invisible",
				[2] = "Start"
			}
		}
		wait()
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
	end
    else
        _G.on = 0
	while _G.on == 1
	do
		local args = {
			[1] = {
				[1] = "Skill_Invisible",
				[2] = "Start"
			}
		}
		wait()
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
	end
    end
end)

