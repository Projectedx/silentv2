if kontol == false then
MFrame:TweenSize(UDim2.new(0, 321, 0, 18),"In","Sine",0,true)
wait(0.1)
Tp.Visible = false
JF2.Visible = false
SF2.Visible = false
SF.Visible = false
JF.Visible = false
BF.Visible = false
PF10.Visible = false
PF.Visible = false
FFC.Visible = false
FFR.Visible = false
FF.Visible = false
TPFrame.Visible = false
kontol = true
else
MFrame:TweenSize(UDim2.new(0, 321, 0, 284),"In","Sine",0,true)
wait(0.1)
Tp.Visible = true
JF2.Visible = true
SF2.Visible = true
SF.Visible = true
JF.Visible = true
BF.Visible = true
PF10.Visible = true
PF.Visible = true
FFC.Visible = true
FFR.Visible = true
FF.Visible = true
kontol = false
end
end)

-- Pemberitahuan ( notify ) --

function notify(msg)
game.StarterGui:SetCore('SendNotification', {
Title = 'Get Rekt';
Text = msg;
Duration = 5;
})
end

